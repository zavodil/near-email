# NEAR network (mainnet or testnet)
NEXT_PUBLIC_NETWORK_ID=mainnet

# OutLayer API URL
NEXT_PUBLIC_OUTLAYER_API_URL=https://outlayer.fastnear.com

# OutLayer project ID
NEXT_PUBLIC_PROJECT_ID=near-email

# Include secrets in OutLayer requests (set to 'false' after master key migration)
NEXT_PUBLIC_USE_SECRETS=true

# Account suffix (near for mainnet, testnet for testnet)
NEXT_PUBLIC_ACCOUNT_SUFFIX=near
